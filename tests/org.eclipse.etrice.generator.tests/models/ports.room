RoomModel base {
	ProtocolClass PTest {
		incoming {
			Message msg()
		}
	}
	
	SubSystemClass CMain {
		ActorRef test: ATest
	}
	
	ActorClass ATest {
		Interface {
			Port relay: PTest
			Port externalEnd: PTest
		}
		Structure {
			Port internalEnd: PTest
			external Port externalEnd
		}
	}
}
